{
  "name": "is-it-hot-in-phx",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js",
    "start:parrot": "concurrently --kill-others 'npm run start -- --env.mock' 'parrot-server --port 3001 --scenarios ./parrot.scenarios.js'",
    "test": "jest"
  },
  "keywords": [
    "parrot",
    "jest-image-snapshot"
  ],
  "author": "Andres Escobar <andres.escobar@aexp.com> (https://github.com/anescobar1991)",
  "license": "Apache-2.0",
  "private": true,
  "devDependencies": {
    "@babel/polyfill": "^7.7.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-amex": "^3.3.0",
    "concurrently": "^5.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^3.0.1",
    "jest-image-snapshot": "^2.11.1",
    "jest-puppeteer-preset": "^4.4.0",
    "node-fetch": "^2.6.1",
    "parrot-server": "^3.1.0",
    "puppeteer": "^2.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "browser"
    ],
    "setupFiles": [
      "<rootDir>/test-setup.js"
    ],
    "projects": [
      "<rootDir>",
      {
        "preset": "jest-puppeteer-preset",
        "testMatch": [
          "**/*.browser.test.js"
        ],
        "setupFiles": [
          "<rootDir>/test-setup.js"
        ]
      }
    ]
  }
}
